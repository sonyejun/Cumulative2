@model Cumulative2.Models.Teacher

@{
    ViewBag.Title = "DeleteConfirm";
}


<div id="containershow">
    <nav>
        <a class="navlink" href="/Teacher/Show/@Model.TeacherId">Cancel</a>
    </nav>
    <h2>Are you sure you want to delete @Model.TeacherFname @Model.TeacherLname?</h2>
    <p>This action cannot be undone.</p>
    <form method="POST" action="#" name="removeTeacher" onsubmit="deleteTeacher(@Model.TeacherId); return false;">
        <button class="delbutton" type="submit">
            Confirm Delete
        </button>
    </form>
</div>